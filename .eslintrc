{
  "parser": "@typescript-eslint/parser",
  "extends": "airbnb-base",
  "env": {
      "es6": true,
      "browser": true
  },
  "rules": {
      "brace-style": [
          "error",
          "stroustrup"
      ],
      "comma-dangle": [
          "error",
          "never"
      ],
      "no-unused-vars": [
          "warn"
      ],
      "no-var": [
          "off"
      ],
      "one-var": [
          "off"
      ],
      "camelcase": [0, {"properties": "never"}]
  },
  "settings": {
    "import/resolver": {
        "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"],
            "moduleDirectory": ["node_modules", "src/"]
        },
    },
}
}
